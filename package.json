{
  "name": "am-forest-codemod",
  "version": "1.0.1",
  "description": "ForestAdmin codemode for agent migration",
  "main": "index.ts",
  "license": "MIT",
  "bin": {
    "forest-codemod": "./bin/run"
  },
  "files": ["dist"],
  "scripts": {
    "mod": "jscodeshift --parser flow -t src/mods/index.ts files-to-mod",
    "debug": "node --inspect ./node_modules/.bin/jscodeshift --parser flow --run-in-band -t src/mods/v7-v8/index.ts files-to-mod",
    "build": "rm -rf dist && tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "jscodeshift": "^0.13.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.5",
    "@types/node": "^17.0.31",
    "oclif": "^3"
  },
  "oclif": {
    "bin": "forest-codemod",
    "dirname": "forest-codemod",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  }
}
